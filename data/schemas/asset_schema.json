        "bear_case": "string|null"
      },
      "competitive_advantage": "string|null",
      "unmet_need": "string|null",
      "source": {
        "id": "string",
        "slide": "number|null",
        "verified": "boolean"
      }
    },
    "competitive_landscape": {
